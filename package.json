{
  "name": "@inkline/nuxt",
  "version": "3.0.0",
  "description": "Nuxt.js Module for Inkline, the intuitive UI UX DX Components library that gives you the foundation for building high-quality, accessible, and customizable Vue.js Design Systems.",
  "main": "index.mjs",
  "module": "index.mjs",
  "scripts": {
    "build": "rimraf lib && tsc -d -p tsconfig.mjs.json && mjscjs -p tsconfig.mjs.json",
    "postbuild": "ts-node scripts/postbuild.ts",
    "lint": "eslint --ext .js,.ts src",
    "pretest": "ts-node scripts/pretest.ts",
    "test": "vitest run",
    "pretest:dev": "npm run pretest",
    "test:dev": "vitest",
    "prerelease": "npm run lint && npm run test && npm run build",
    "release": "np",
    "postrelease": "ts-node scripts/postrelease.ts",
    "postversion": "ts-node scripts/postversion.ts"
  },
  "files": [
    "**/*"
  ],
  "exports": {
    ".": {
      "import": "./index.mjs",
      "types": "./index.d.ts"
    },
    "./*": "./*"
  },
  "dependencies": {
    "@nuxt/kit": "0.8.1-edge"
  },
  "peerDependencies": {
    "sass": "*",
    "@inkline/inkline": ">=3.0.0",
    "vue": ">=3.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/inkline/nuxt-module-inkline"
  },
  "keywords": [
    "inkline",
    "nuxt",
    "module",
    "vue"
  ],
  "devDependencies": {
    "@types/node": "17.0.10",
    "@types/shelljs": "0.8.11",
    "@typescript-eslint/eslint-plugin": "5.10.0",
    "@typescript-eslint/parser": "5.10.0",
    "eslint": "8.7.0",
    "eslint-config-standard": "16.0.3",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "6.0.0",
    "mjscjs": "1.0.9",
    "np": "7.6.0",
    "rimraf": "3.0.2",
    "shelljs": "0.8.5",
    "ts-node": "10.4.0",
    "typescript": "4.5.4",
    "vite": "2.7.13",
    "vitest": "0.1.21"
  },
  "license": "MIT",
  "gitHead": "f1538ce4fc953d4090885c0d023bb357e2d1a2b7"
}
